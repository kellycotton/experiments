        <Param val="" valType="extendedCode" updates="constant" name="Begin JS Experiment"/>
        <Param val="&amp;#10;        // add-on: list(s: string): string[]&amp;#10;        function list(s) {&amp;#10;            // if s is a string, we return a list of its characters&amp;#10;            if (typeof s === 'string')&amp;#10;                return s.split('');&amp;#10;            else&amp;#10;                // otherwise we return s:&amp;#10;                return s;&amp;#10;        }&amp;#10;range = function (size, startAt = 0) {&amp;#10;    return [...Array(size).keys()].map(i =&gt; i + startAt);&amp;#10;}    &amp;#10;&amp;#10;//import * as re from 're';&amp;#10;//var angles, block_n, block_total, circle_size, dot_pos_resp, dot_size, fix_time, i, image_size, k, memory_items, original_imgs, parity_time, points_total, positions, stim_array_time, trial_n, trial_total;&amp;#10;positions = [[(- 0.25), 0], [(- 0.25), 0.25], [0, 0.25], [0.25, 0.25], [0.25, 0], [0.25, (- 0.25)], [0, (- 0.25)], [(- 0.25), (- 0.25)]];&amp;#10;circle_size = [0.2, 0.2];&amp;#10;dot_size = [0.05, 0.05];&amp;#10;angles = Array.from({length: 360}, (_, i) =&gt; i + 1)&amp;#10;dot_pos_resp = [0, 0];&amp;#10;//original_imgs = Array(540).fill(&quot;&quot;);&amp;#10;original_imgs = Array(200).fill(&quot;&quot;);&amp;#10;fix_time = 0.5;&amp;#10;stim_array_time = 0.75;&amp;#10;parity_time = 1.2;&amp;#10;image_size = [0.05, 0.05];&amp;#10;block_n = 1;&amp;#10;trial_n = 1;&amp;#10;block_total = 4;&amp;#10;wm_trial_total = 4;&amp;#10;memory_items = [];&amp;#10;points_total = 0;&amp;#10;i = 1;&amp;#10;k = 1;&amp;#10;for (var k, _pj_c = 0, _pj_a = range(200), _pj_b = _pj_a.length; (_pj_c &lt; _pj_b); _pj_c += 1) {&amp;#10;    k = _pj_a[_pj_c];&amp;#10;    original_imgs[k] = ('object_stim/obj' + k.toString() + '.jpg');&amp;#10;}&amp;#10;&amp;#10;function scalar_to_rgb(scalar) {&amp;#10;    var color_list;&amp;#10;    color_list = colorsys.hsv_to_rgb(scalar, 1, 1);&amp;#10;    color_list = function () {&amp;#10;    var _pj_a = [], _pj_b = color_list;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var element = _pj_b[_pj_c];&amp;#10;        _pj_a.push((element * 255));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;    return color_list;&amp;#10;}&amp;#10;&amp;#10;util.shuffle(original_imgs);&amp;#10;document.body.style.cursor='none';&amp;#10;" valType="extendedCode" updates="constant" name="Begin JS Routine"/>
        <Param val="&amp;#10;positions = [[(- 0.25), 0], [(- 0.25), 0.25], [0, 0.25], [0.25, 0.25], [0.25, 0], [0.25, (- 0.25)], [0, (- 0.25)], [(- 0.25), (- 0.25)]]&amp;#10;circle_size = [0.2, 0.2]&amp;#10;dot_size = [0.05, 0.05]&amp;#10;angles = list(range(1,361))&amp;#10;dot_pos_resp = [0, 0]&amp;#10;original_imgs = [[]]*200&amp;#10;fix_time = 0.5&amp;#10;stim_array_time = 0.75&amp;#10;parity_time = 1.2&amp;#10;image_size = [0.05, 0.05]&amp;#10;block_n = 1&amp;#10;trial_n = 1&amp;#10;block_total = 4&amp;#10;wm_trial_total = 4&amp;#10;memory_items = []&amp;#10;points_total = 0&amp;#10;i = 1&amp;#10;k = 1&amp;#10;for k in range(200):&amp;#10;    original_imgs[k] = &quot;object_stim/obj{}.jpg&quot;.format(k)&amp;#10;&amp;#10;shuffle(original_imgs)&amp;#10;win.mouseVisible = False&amp;#10;" valType="extendedCode" updates="constant" name="Begin Routine"/>
