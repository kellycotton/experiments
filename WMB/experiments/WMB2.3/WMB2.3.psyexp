        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="shape_1 = 0&amp;#10;color_1 = 0&amp;#10;shape_2 = 0&amp;#10;color_2 = 0&amp;#10;&amp;#10;if ((condition === &quot;same&quot;)) {&amp;#10;    stimuli_1 = stimuli[0];&amp;#10;    correct_answer = &quot;s&quot;;&amp;#10;} else {&amp;#10;    if ((block === &quot;shape_and_color&quot;)) {&amp;#10;        shape_1 = get_shape_from_filename(stimuli[0]);&amp;#10;        color_2 = get_color_from_filename(stimuli[1]);&amp;#10;        stimuli_1 = get_filename_from_shape_and_color(shape_1, color_2)&amp;#10;    } else if ((block === &quot;shape_or_color&quot;)) {&amp;#10;        if (condition_change_order[trial_counter] === &quot;shape&quot;) {&amp;#10;            change = &quot;shape&quot;;&amp;#10;            color_1 = get_color_from_filename(stimuli[0]);&amp;#10;            stimuli_1 = get_filename_from_shape_and_color(possible_shapes[6], color_1)&amp;#10;            } &amp;#10;        else {&amp;#10;            change = &quot;color&quot;;&amp;#10;            shape_1 = get_shape_from_filename(stimuli[0]);&amp;#10;            stimuli_1 = get_filename_from_shape_and_color(shape_1, possible_colors[6])&amp;#10;            } &amp;#10;        } else {&amp;#10;        stimuli_1 = stimuli[6];&amp;#10;    }&amp;#10;    correct_answer = &quot;d&quot;;&amp;#10;}" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="if setsize ==3:&amp;#10;    pos = [stim1.pos, stim2.pos, stim3.pos]&amp;#10;    test_positions = get_test_positions(pos)&amp;#10;    test_positions.extend([stim4.pos, stim5.pos, stim6.pos])&amp;#10;else: &amp;#10;    pos = [stim1.pos, stim2.pos, stim3.pos, stim4.pos, stim5.pos, stim6.pos]&amp;#10;    test_positions = get_test_positions(pos)&amp;#10;&amp;#10;if condition == &quot;same&quot;:&amp;#10;    stimuli_1 = stimuli[0]&amp;#10;    stimuli_2 = stimuli[1]&amp;#10;    correct_answer = 's'&amp;#10;else:&amp;#10;    stimuli_1 = stimuli[6]&amp;#10;    stimuli_2 = stimuli[7]&amp;#10;    correct_answer = 'd'&amp;#10;" valType="extendedCode"/>
