        <Param name="Begin JS Routine" updates="constant" val="if ((block === &quot;shape&quot;)) {&amp;#10;    stimuli = shapes;&amp;#10;    shuffle(stimuli);&amp;#10;} else if ((block == &quot;color&quot;)){&amp;#10;    stimuli = colors;&amp;#10;    shuffle(stimuli);&amp;#10;} else {&amp;#10;        shuffle(possible_colors);&amp;#10;        shuffle(possible_shapes);&amp;#10;        stimuli = [get_filename_from_shape_and_color(possible_shapes[0],possible_colors[0]),&amp;#10;        get_filename_from_shape_and_color(possible_shapes[1],possible_colors[1]),&amp;#10;        get_filename_from_shape_and_color(possible_shapes[2],possible_colors[2]),&amp;#10;        get_filename_from_shape_and_color(possible_shapes[3],possible_colors[3])]&amp;#10;    }&amp;#10;&amp;#10;shuffle(positions);&amp;#10;if ((setsize === 4)) {&amp;#10;    visibility = 1;&amp;#10;} else {&amp;#10;    visibility = 0;&amp;#10;}&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="if block == &quot;shape&quot;:&amp;#10;    stimuli = shapes&amp;#10;#elif block == &quot;color&quot;:&amp;#10;else:&amp;#10;    stimuli = colors&amp;#10;#elif block == &quot;shape_or_color&quot;:&amp;#10;#    stimuli = &amp;#10;#else:&amp;#10;#    stimuli = &amp;#10;&amp;#10;shuffle(stimuli)&amp;#10;shuffle(positions)&amp;#10;if setsize == 6:&amp;#10;    visibility = 1&amp;#10;else:&amp;#10;    visibility = 0&amp;#10;" valType="extendedCode"/>
